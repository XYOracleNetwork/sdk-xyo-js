{
  "packageManager": "yarn@3.2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/mocha": "^9.1.1",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/rollup-config": "^1.1.17",
    "@xylabs/ts-scripts": "^1.0.55",
    "@xylabs/tsconfig": "^1.0.13",
    "babel-jest": "^28.0.2",
    "copyfiles": "^2.4.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.14.0",
    "jest": "^28.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "ts-jest": "^28.0.0-next.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "xylabs-build": "echo Building all",
    "build": "yarn workspaces foreach run xylabs-build",
    "deploy3": "yarn cycle && yarn build && yarn prep-version",
    "prep-version": "yarn workspaces foreach run version apply --all && yarn workspaces foreach run version patch --deferred",
    "cycle": "yarn workspaces foreach run xylabs-cycle",
    "lint": "yarn workspaces foreach run xylabs-lint",
    "fix": "yarn workspaces foreach run xylabs-fix",
    "test": "yarn workspaces foreach run xylabs-test",
    "deps": "yarn workspaces foreach run xylabs-deps"
  },
  "version": "2.6.2"
}
