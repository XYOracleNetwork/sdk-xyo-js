{
  "name": "@xyo-network/sdk-xyo-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "esm5": "dist/esm5/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm5/index.js",
      "node": {
        "import": "./dist/node-esm/index.js",
        "require": "./dist/node/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "license": "LGPL-3.0",
  "main": "dist/node/index.js",
  "module": "dist/index.js",
  "scripts": {
    "compile": "rm -r -f dist && yarn compile-rollup && yarn compile-tsc",
    "compile-rollup": "rollup -c",
    "compile-tsc": "tsc -p tsconfig.json",
    "lint-pkg": "npmPkgJsonLint .",
    "test": "jest"
  },
  "resolutions": {
    "uuid": "^8"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27",
    "@types/mocha": "^9",
    "@xylabs/eslint-config": "^2",
    "@xylabs/rollup-config": "^1",
    "@xylabs/ts-scripts": "^1",
    "@xylabs/tsconfig": "^1",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "npm-package-json-lint": "^5.4.2",
    "npm-package-json-lint-config-default": "^3.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "version": "2.4.0"
}