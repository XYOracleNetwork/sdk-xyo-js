{
  "name": "@xyo-network/sdk-xyo-js",
  "version": "2.6.21",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "browser": "dist/esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "LGPL-3.0",
  "packageManager": "yarn@3.2.1",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@xyo-network/ethers-wrappers": "workspace:^",
    "@xyo-network/sdk-geo": "workspace:^",
    "@xyo-network/sdk-meta": "workspace:^"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/mocha": "^9.1.1",
    "@xylabs/eslint-config": "^2.5.65",
    "@xylabs/rollup-config": "^1.1.17",
    "@xylabs/ts-scripts-yarn3": "^2.5.65",
    "@xylabs/tsconfig": "^2.5.65",
    "jest": "^28.1.3",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "gen:docs": "yarn workspaces foreach -pv run gen:docs:package",
    "build-storybook": "build-storybook ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "outdated": "yarn workspaces foreach -pv outdated"
  },
  "sideEffects": false
}
