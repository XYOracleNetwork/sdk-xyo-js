{
  "name": "@xyo-network/sdk-xyo-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "version": "2.1.7",
  "main": "./index.cjs.js",
  "module": "./index.js",
  "types": "./index.d.js",
  "exports": {
    ".": "./index.js",
    "./assertEx": "./assertEx.js"
  },
  "sideEffects": false,
  "peerDependencies": {
    "axios": "^0.21.1",
    "rollbar": "^2.21.1"
  },
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn compile && yarn copy-src",
    "copy-src": "cp package.json ./dist && cp -R ./src/* ./dist",
    "compile": "rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish ./dist --access public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json ./src",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx,.json ./src --fix",
    "test": "jest",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./dist && rm -r -f ./build",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck --ignores \"vue, @types/jest, ts-node\""
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "depcheck": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.47.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-prune": "^0.9.0",
    "typescript": "~4.2.2",
    "vue": "^3.0.11"
  }
}
