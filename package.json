{
  "name": "@xyo-network/sdk-xyo-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "files": [
    "./build"
  ],
  "license": "LGPL-3.0",
  "private": false,
  "version": "2.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "main": "./build/index.cjs.js",
  "module": "./build/index.esm.js",
  "types": "./build/index.d.js",
  "sideEffects": false,
  "dependencies": {
    "axios": "^0.21.1",
    "rollbar": "^2.21.1"
  },
  "scripts": {
    "build": "yarn build:ci",
    "build:ci": "yarn lint && rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn publish --access public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./build",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "depcheck": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "eslint-plugin-workspaces": "^0.6.2",
    "prettier": "^2.2.1",
    "rollup": "^2.42.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-prune": "^0.9.0",
    "typescript": "~4.2.2",
    "vue": "^3.0.11"
  }
}
