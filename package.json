{
  "name": "@xyo-network/sdk-xyo-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "esm5": "dist/esm5/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "esm5": "./dist/esm5/index.js",
      "node": {
        "import": "./dist/node-esm/index.js",
        "require": "./dist/node/index.js"
      },
      "default": "./dist/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "license": "LGPL-3.0",
  "main": "dist/node/index.js",
  "module": "dist/index.js",
  "scripts": {
    "deploy": "yarn deploy3",
    "lint-pkg": "npmPkgJsonLint .",
    "build": "yarn xylabs-build && yarn gen:docs",
    "gen:docs": "typedoc ./src/index.ts --json ./dist/docs.json --tsconfig ./tsconfig.build.json",
    "test": "jest ./src"
  },
  "resolutions": {
    "uuid": "^8"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "geojson": "^0.5.0",
    "mapbox-gl": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/mapbox-gl": "^2.7.0",
    "@types/mocha": "^9.1.1",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/rollup-config": "^1.1.15",
    "@xylabs/ts-scripts": "^1.0.53",
    "@xylabs/tsconfig": "^1.0.13",
    "copyfiles": "^2.4.1",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "ts-jest": "^27.1.4",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "version": "2.5.6",
  "packageManager": "yarn@3.2.0"
}
