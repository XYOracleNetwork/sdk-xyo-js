{
  "name": "@xyo-network/sdk-xyo-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/nodeIndex.js",
        "require": "./dist/cjs/nodeIndex.js"
      },
      "browser": {
        "import": "./dist/esm/browserIndex.js",
        "require": "./dist/esm/browserIndex.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "license": "LGPL-3.0",
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@xyo-network/sdk-geo": "workspace:^",
    "@xyo-network/sdk-meta": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@types/jest": "^27.5.0",
    "@types/mocha": "^9.1.1",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/rollup-config": "^1.1.17",
    "@xylabs/ts-scripts": "^1.0.65",
    "@xylabs/tsconfig": "^1.0.13",
    "babel-jest": "^28.0.3",
    "copyfiles": "^2.4.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.14.0",
    "jest": "^28.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.72.0",
    "ts-jest": "^28.0.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "build-all": "yarn workspaces foreach run build && yarn workspaces foreach version patch --deferred",
    "build": "yarn clean && tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json && yarn version patch --deferred",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach version patch --deferred && yarn deploy-common",
    "deploy-common": "yarn cycle && yarn workspaces foreach run build && yarn version apply --all && yarn workspaces foreach npm publish",
    "cycle": "yarn workspaces foreach run cycle",
    "lint": "yarn workspaces foreach run lint",
    "fix": "yarn workspaces foreach run fix",
    "test": "yarn workspaces foreach run test",
    "deps": "yarn workspaces foreach run deps"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "2.6.17"
}
