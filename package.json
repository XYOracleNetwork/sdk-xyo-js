{
  "name": "@xyo-network/sdk-xyo-js",
  "version": "2.6.21",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "browser": "dist/esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "LGPL-3.0",
  "packageManager": "yarn@3.2.1",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@xyo-network/ethers-wrappers": "workspace:^",
    "@xyo-network/sdk-geo": "workspace:^",
    "@xyo-network/sdk-meta": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/rollup-config": "^1.1.17",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "babel-jest": "^28.1.1",
    "copyfiles": "^2.4.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "ts-jest": "^28.0.4",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn gen:docs",
    "build:cjs": "yarn workspaces foreach -pRtv run build:package:cjs",
    "build:esm": "yarn workspaces foreach -pRtv run build:package:esm",
    "gen:docs": "yarn workspaces foreach -pv run gen:docs:package",
    "build-storybook": "build-storybook ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach --all version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach --all version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach --all version patch --deferred && yarn deploy-common",
    "deploy-common": "yarn lint && yarn build && yarn gen:docs && yarn version apply --all && yarn workspaces foreach -ptv npm publish",
    "clean": "yarn workspaces foreach -pRtv run clean:package",
    "deps": "yarn workspaces foreach -pRtv run deps:package",
    "lint": "yarn workspaces foreach -pRtv run lint:package",
    "fix": "yarn workspaces foreach -pRtv run fix:package",
    "outdated": "yarn workspaces foreach -pv outdated",
    "up": "yarn workspaces foreach -pv up",
    "updo": "yarn workspaces foreach -pv run updo3:package",
    "gen:docs:package": "typedoc $INIT_CWD/src/index.ts --json $INIT_CWD/dist/docs.json",
    "clean:package": "rimraf -q $INIT_CWD/dist && rimraf -q $INIT_CWD/build",
    "fix:package": "eslint $INIT_CWD/src --fix",
    "lint:package": "eslint $INIT_CWD/src",
    "deps:package": "depcheck $INIT_CWD",
    "updo3:package": "yarn update-interactive",
    "build:package:cjs": "tsc -p $INIT_CWD/tsconfig.build.cjs.json",
    "build:package:esm": "tsc -p $INIT_CWD/tsconfig.build.esm.json"
  },
  "sideEffects": false
}