{
  "name": "@xyo-network/sdk-xyo-js",
  "version": "2.8.0",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "browser": "dist/esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "license": "LGPL-3.0",
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@xyo-network/ethers-wrappers": "2.6.17",
    "@xyo-network/sdk-geo": "workspace:^"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "@xylabs/eslint-config": "^2.14.3",
    "@xylabs/ts-scripts-yarn3": "^2.14.3",
    "@xylabs/tsconfig": "^2.14.3",
    "@xylabs/tsconfig-jest": "^2.14.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "jest": "^29.4.2",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "uuid": "^8"
  },
  "sideEffects": false
}
