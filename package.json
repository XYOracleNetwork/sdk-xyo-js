{
  "name": "@xyo-network/sdk-xyo-js",
  "version": "2.10.0",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.cjs"
      },
      "browser": {
        "import": "./dist/browser/index.js",
        "require": "./dist/browser/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "license": "LGPL-3.0",
  "packageManager": "yarn@3.5.1",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@xyo-network/ethers-wrappers": "workspace:^",
    "@xyo-network/sdk-geo": "workspace:^"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/mocha": "^10.0.2",
    "@xylabs/eslint-config": "^3.0.82",
    "@xylabs/ts-scripts-yarn3": "^3.0.82",
    "@xylabs/tsconfig": "^3.0.82",
    "@xylabs/tsconfig-jest": "^3.0.82",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "uuid": "^8",
    "postcss": "^8.4.31"
  },
  "scripts": {
    "test": "yarn jest",
    "build-typedoc-site": "typedoc --options typedoc.site.json"
  },
  "sideEffects": false,
  "type": "module"
}