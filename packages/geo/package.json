{
  "name": "@xyo-network/sdk-geo",
  "version": "2.7.0",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "esm"
  ],
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "license": "LGPL-3.0",
  "dependencies": {
    "geojson": "^0.5.0",
    "mapbox-gl": "^2.12.1"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^2.7.10",
    "@xylabs/ts-scripts-yarn3": "^2.14.3",
    "@xylabs/tsconfig": "^2.14.3"
  },
  "sideEffects": false,
  "packageManager": "yarn@3.2.0"
}