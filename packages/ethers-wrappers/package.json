{
  "name": "@xyo-network/ethers-wrappers",
  "version": "2.10.0",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Ethers wrappers for basic contracts",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "xyo",
    "utility",
    "ethers",
    "typescript",
    "esm"
  ],
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.cjs"
      },
      "browser": {
        "import": "./dist/browser/index.js",
        "require": "./dist/browser/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "license": "LGPL-3.0",
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@xylabs/sdk-js": "^2.12.14",
    "@xyo-network/typechain": "^2.2.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.0.80",
    "@xylabs/tsconfig": "^3.0.80",
    "ethers": "^5.7.2"
  },
  "sideEffects": false,
  "packageManager": "yarn@3.2.0",
  "type": "module"
}
